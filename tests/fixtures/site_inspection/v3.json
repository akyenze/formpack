{
  "id_string": "site_inspection",
  "version": "siv3",
  "content": {
    "survey": [
      {
        "required": "true",
        "type": "text",
        "name": "inspector",
        "label": "inspector"
      },
      {
        "required": "true",
        "type": "select_one zero_one",
        "name": "did_you_find_the_site",
        "label": "did you find the site"
      },
      {
        "relevant": "${did_you_find_the_site} = 'yes'",
        "type": "begin group",
        "name": "group_site_condition",
        "label": "site condition"
      },
        {
          "required": "true",
          "type": "select_one zero_one",
          "name": "was_there_damage_to_the_site",
          "label": "was there damage to the site?"
        },
        {
          "type": "begin group",
          "name": "group_proprietary_tool",
          "label": "proprietary tool name"
        },
          {
            "required": "true",
            "type": "integer",
            "name": "ping",
            "label": "ping"
          },
          {
            "required": "true",
            "type": "integer",
            "name": "rssi",
            "label": "rssi"
          },
        {
          "type": "end group"
        },
        {
          "type": "begin group",
          "name": "group_gate",
          "label": "gate"
        },
          {
            "required": "true",
            "type": "select_one zero_one",
            "name": "is_the_gate_secure",
            "label": "is the gate secure?"
          },
        {
          "type": "end group"
        },
        {
          "required": "true",
          "type": "select_one zero_one",
          "name": "is_plant_life_encroaching",
          "label": "is plant life encroaching?"
        },
      {
        "type": "end group"
      },
      {
        "required": "true",
        "type": "select_one l_m_s",
        "name": "please_rate_the_impact_of_any_defects_observed",
        "label": "please rate the impact of any defects observed"
      }
    ],
    "choices": [
      {
        "list_name": "zero_one",
        "name": 0,
        "label": "no"
      },
      {
        "list_name": "zero_one",
        "name": 1,
        "label": "yes"
      },
      {
        "list_name": "l_m_s",
        "name": "low",
        "label": "low"
      },
      {
        "list_name": "l_m_s",
        "name": "moderate",
        "label": "moderate"
      },
      {
        "list_name": "l_m_s",
        "name": "severe",
        "label": "severe"
      }
    ]
  },
